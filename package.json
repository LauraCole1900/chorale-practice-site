{
  "name": "gcpm",
  "version": "1.2.0",
  "description": "Greeley Chorale Practice Materials",
  "author": "Laura E. Cole",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js --openssl-legacy-provider start",
